{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@wolfstar-project/dashboard",
  "version": "2.0.0",
  "private": true,
  "description": "Wolfstar Dashboard",
  "homepage": "https://wolfstar.rocks",
  "bugs": {
    "url": "https://github.com/wolfstar-project/wolfstar.rocks/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wolfstar-project/wolfstar.rocks.git"
  },
  "funding": "https://github.com/wolfstar-project/wolfstar.rocks?sponsor",
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "pnpm prisma:generate && nuxi build",
    "deploy": "pnpx nuxthub deploy",
    "dev": "pnpm prisma:generate && nuxi dev",
    "dev:pwa": "cross-env VITE_PLUGIN_PWA=true pnpm prisma:generate && nuxi dev",
    "dev:stagewise": "pnpm dlx stagewise@latest -p 3100 -a 3000 -b -- nuxi dev",
    "format": "prettier --write .",
    "generate": "nuxt generate",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "postinstall": "stale-dep -u && nuxt prepare",
    "prepare": "node .husky/install.mjs",
    "preview": "pnpx nuxthub preview",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:dev:create": "prisma migrate dev --create-only",
    "prisma:migrate:diff": "prisma migrate diff --exit-code --from-migrations prisma/migrations --to-schema-datamodel prisma/schema/schema.prisma --shadow-database-url",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:status": "prisma migrate status",
    "prisma:migrate:resolve": "prisma migrate resolve",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "serve": "node .output/server/index.mjs",
    "typecheck": "stale-dep && nuxt typecheck"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": "eslint --fix ."
  },
  "dependencies": {
    "@ai-sdk/vue": "catalog:dev",
    "@josephanson/nuxt-ai": "catalog:prod",
    "@nuxt/scripts": "catalog:dev",
    "@nuxtjs/color-mode": "catalog:prod",
    "@pinia/nuxt": "catalog:frontend",
    "@prisma/adapter-pg": "catalog:prod",
    "@prisma/client": "catalog:prod",
    "@sentry/nuxt": "catalog:dev",
    "@tailwindcss/vite": "catalog:build",
    "@tanstack/pacer": "catalog:frontend",
    "@vee-validate/zod": "catalog:utils",
    "@vite-pwa/nuxt": "catalog:build",
    "@vueuse/core": "catalog:utils",
    "@vueuse/nuxt": "catalog:utils",
    "@vueuse/router": "catalog:utils",
    "clsx": "catalog:utils",
    "colortranslator": "catalog:prod",
    "consola": "catalog:node",
    "cross-env": "catalog:cli",
    "dotenv": "catalog:node",
    "fuse.js": "catalog:prod",
    "import-in-the-middle": "catalog:prod",
    "markdown-it-anchor": "catalog:markdown",
    "motion-v": "catalog:frontend",
    "nitro-cloudflare-dev": "catalog:dev",
    "nuxt": "catalog:prod",
    "nuxt-authorization": "catalog:prod",
    "nuxt-security": "catalog:prod",
    "nuxt-vue-dragscroll": "catalog:frontend",
    "pinia": "catalog:frontend",
    "postcss-nested": "catalog:style",
    "postcss-nesting": "catalog:style",
    "prisma": "catalog:dev",
    "prisma-json-types-generator": "catalog:prod",
    "reka-ui": "catalog:frontend",
    "shadcn-nuxt": "catalog:prod",
    "simple-git": "catalog:dev",
    "stale-dep": "catalog:prod",
    "tailwind-merge": "catalog:style",
    "tailwind-variants": "catalog:prod",
    "taze": "catalog:cli",
    "tw-animate-css": "catalog:style",
    "utility-types": "catalog:prod",
    "vue-markdown-render": "catalog:markdown",
    "wrangler": "catalog:dev",
    "zod": "catalog:utils",
    "yup": "catalog:utils",
    "@vee-validate/yup": "catalog:utils"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:lint",
    "@antfu/nip": "catalog:dev",
    "@commitlint/cli": "catalog:lint",
    "@commitlint/config-conventional": "catalog:lint",
    "@discordjs/collection": "catalog:dev",
    "@discordjs/core": "catalog:dev",
    "@iconify-json/heroicons": "catalog:icons",
    "@iconify-json/ic": "catalog:icons",
    "@iconify-json/lucide": "catalog:icons",
    "@iconify-json/ph": "catalog:icons",
    "@nuxt/eslint": "catalog:lint",
    "@nuxt/fonts": "catalog:dev",
    "@nuxt/icon": "catalog:icons",
    "@nuxt/image": "catalog:dev",
    "@nuxthub/core": "catalog:dev",
    "@nuxtjs/seo": "catalog:dev",
    "@sapphire/bitfield": "catalog:dev",
    "@sapphire/prettier-config": "catalog:lint",
    "@sapphire/result": "catalog:dev",
    "@sapphire/snowflake": "catalog:dev",
    "@sapphire/time-utilities": "catalog:dev",
    "@sapphire/utilities": "catalog:dev",
    "@tailwindcss/typography": "catalog:style",
    "@tailwindcss/vite": "catalog:build",
    "daisyui": "catalog:dev",
    "discord-api-types": "catalog:dev",
    "eslint": "catalog:lint",
    "eslint-plugin-format": "catalog:lint",
    "eslint-plugin-package-json": "catalog:lint",
    "husky": "catalog:lint",
    "lint-staged": "catalog:lint",
    "nuxt-auth-utils": "catalog:dev",
    "playwright": "catalog:test",
    "pncat": "catalog:cli",
    "prettier": "catalog:lint",
    "prettier-plugin-css-order": "catalog:lint",
    "prettier-plugin-tailwindcss": "catalog:lint",
    "std-env": "catalog:dev",
    "tailwindcss": "catalog:style",
    "typescript": "catalog:dev",
    "vue-tsc": "catalog:dev"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "volta": {
    "node": "22.17.0"
  }
}
