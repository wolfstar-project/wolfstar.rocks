{
  "servers": {
    "nuxt": {
      "type": "sse",
      "url": "http://localhost:3000/__mcp/sse"
    },
    "eslint": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@eslint/mcp"
      ]
    },
    "browsermcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@browser/mcp@latest"
      ]
    },
    "nuxt-ui": {
      "type": "http",
      "url": "https://ui.nuxt.com/mcp"
    },
    "sentry": {
      "type": "http",
      "url": "https://mcp.sentry.dev/mcp"
    }
  },
  "inputs": []
}
